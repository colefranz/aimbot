{
  "name": "Aimbot",
  "version": "0.0.6",
  "manifest_version": 2,
  "description": "Become a human aimbot",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "Open Aimbot",
    "default_popup": "dist/index.html"
  },
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
