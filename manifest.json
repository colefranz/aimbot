{
  "name": "Aimbot",
  "version": "0.0.5",
  "manifest_version": 2,
  "description": "Become a human aimbot",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "Open Aimbot",
    "default_popup": "build/aimbot.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
